{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/dev",
    "pipeline": {
        "build": {
            "dependsOn": ["lint", "clear", "^build"],
            "outputs": ["dist/**"],
            "cache": false
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "clear": {
            "cache": false
        },
        "lint": {
            "outputs": []
        },
        "lintfix": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "start": {
            "cache": false
        }
    }
}
